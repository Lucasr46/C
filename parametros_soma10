/******************************************************************************
Lucas Henrique Ramos
Nesse programa criamos uma função SOMA10. Somará + 10 com o valor que o usuário inserir.
*******************************************************************************/

#include <stdio.h>

void soma10(int *x){

*x = 10 + *x;

return ;
}

int main(){
    
    int numero;
    
    printf("Insira o valor do numero: ");
    scanf ("%d", &numero);
    
    soma10(&numero);
    
    printf("O novo valor é: %d", numero);
    
    return 0;
}
